{
  "name": "punktde/varnish",
  "type": "typo3-cms-extension",
  "description": "punkt.de fork of varnish",
  "keywords": [
    "TYPO3",
    "extension"
  ],
  "homepage": "http://punkt.de",
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": ">=6.2.0,<9.0"
  },
  "autoload": {
    "classmap": [
      "Classes/"
    ]
  },
  "replace": {
    "varnish": "self.version",
    "typo3-ter/varnish": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
